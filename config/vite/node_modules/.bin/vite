#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/alexgrozav/Workspace/styleframe/node_modules/.pnpm/vite@7.0.6_@types+node@22.15.32_jiti@2.4.2_lightningcss@1.30.1_terser@5.43.1_yaml@2.8.0/node_modules/vite/bin/node_modules:/Users/alexgrozav/Workspace/styleframe/node_modules/.pnpm/vite@7.0.6_@types+node@22.15.32_jiti@2.4.2_lightningcss@1.30.1_terser@5.43.1_yaml@2.8.0/node_modules/vite/node_modules:/Users/alexgrozav/Workspace/styleframe/node_modules/.pnpm/vite@7.0.6_@types+node@22.15.32_jiti@2.4.2_lightningcss@1.30.1_terser@5.43.1_yaml@2.8.0/node_modules:/Users/alexgrozav/Workspace/styleframe/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/alexgrozav/Workspace/styleframe/node_modules/.pnpm/vite@7.0.6_@types+node@22.15.32_jiti@2.4.2_lightningcss@1.30.1_terser@5.43.1_yaml@2.8.0/node_modules/vite/bin/node_modules:/Users/alexgrozav/Workspace/styleframe/node_modules/.pnpm/vite@7.0.6_@types+node@22.15.32_jiti@2.4.2_lightningcss@1.30.1_terser@5.43.1_yaml@2.8.0/node_modules/vite/node_modules:/Users/alexgrozav/Workspace/styleframe/node_modules/.pnpm/vite@7.0.6_@types+node@22.15.32_jiti@2.4.2_lightningcss@1.30.1_terser@5.43.1_yaml@2.8.0/node_modules:/Users/alexgrozav/Workspace/styleframe/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
