{
	"name": "@styleframe/cli",
	"version": "1.0.0",
	"description": "A command-line interface for styleframe.",
	"type": "module",
	"bin": {
		"styleframe": "./dist/index.umd.cjs"
	},
	"types": "./dist/index.d.ts",
	"module": "./dist/index.js",
	"main": "./dist/index.umd.cjs",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.umd.cjs"
		}
	},
	"scripts": {
		"dev": "vite build --watch",
		"prebuild": "tsx ./scripts/prebuild.ts",
		"build": "pnpm run prebuild && pnpm typecheck && vite build",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:dev": "vitest --watch",
		"playground:init": "tsx src/playground/init.ts",
		"playground:build": "tsx src/playground/build.ts"
	},
	"dependencies": {
		"@styleframe/loader": "workspace:^",
		"citty": "^0.1.6",
		"consola": "^3.0.0-2"
	},
	"devDependencies": {
		"@styleframe/config-typescript": "workspace:^",
		"@styleframe/config-vite": "workspace:^",
		"tsx": "catalog:",
		"typescript": "catalog:",
		"vite": "catalog:",
		"vite-plugin-dts": "catalog:",
		"vite-plugin-node": "catalog:",
		"vitest": "catalog:"
	}
}
